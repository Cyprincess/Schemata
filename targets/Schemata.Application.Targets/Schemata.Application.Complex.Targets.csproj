<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Authors>Chino Chang</Authors>
    <Description>Schemata Application Framework - Comprehensive Application Project Targets</Description>
    <PackageTags>application;business;dsl;database;datasource;framework;generator;modular;repository;tenancy</PackageTags>
  </PropertyGroup>

  <PropertyGroup>
    <UseDSLTargets>true</UseDSLTargets>
